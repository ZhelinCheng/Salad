{
  "name": "salad",
  "version": "2.0.1",
  "description": "美女占位图，前端占位图生成，超强的占位图程序。",
  "author": "Zhelin Cheng",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && node ./dist/bin/generate.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=test nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "7.5.4",
    "@nestjs/core": "7.5.4",
    "@nestjs/platform-express": "7.5.4",
    "@types/fs-extra": "9.0.4",
    "@types/lowdb": "1.0.9",
    "@types/sharp": "0.26.1",
    "cache-manager": "3.4.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "cross-env": "7.0.2",
    "dotenv": "8.2.0",
    "express-rate-limit": "5.2.2",
    "fs-extra": "9.0.1",
    "helmet": "4.2.0",
    "image-size": "0.9.3",
    "lowdb": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3",
    "text-to-svg": "3.1.5",
    "ts-md5": "1.2.7",
    "minipass": "2.7.0",
    "sharp": "0.26.3"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.3",
    "@nestjs/schematics": "7.2.2",
    "@nestjs/testing": "7.5.4",
    "@types/express": "4.17.9",
    "@types/jest": "26.0.15",
    "@types/node": "12.12.31",
    "@types/supertest": "2.0.10",
    "jest": "26.6.3",
    "prettier": "2.1.2",
    "supertest": "6.0.1",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.11",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.3",
    "typescript": "4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
